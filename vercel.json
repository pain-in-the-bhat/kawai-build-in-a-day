{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/generate-project-idea",
        "dest": "/api/generate-project-idea"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  